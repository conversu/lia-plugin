{
  "name": "webpack-bundle-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build:app": "rm -rf ./build && react-scripts build",
    "bundle:dev": "webpack --env development env=development --mode development --progress",
    "bundle:stage": "webpack --env stage env=stage --mode stage --progress",
    "bundle:prod": "webpack --env production env=production --mode production --progress",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@conversu/chat": "^1.0.1",
    "@conversu/commons": "^3.2.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@tanstack/react-query": "^4.36.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.5",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.3.31",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.5",
    "uuid": "^10.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@types/uuid": "^10.0.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "file-loader": "^6.2.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack-cli": "^5.1.4"
  }
}
